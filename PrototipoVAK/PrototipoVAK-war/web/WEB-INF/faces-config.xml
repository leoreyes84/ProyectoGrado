<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
        <locale-config>
            <default-locale>es</default-locale>
        </locale-config>
        <resource-bundle>
            <base-name>co.edu.udistrital.prototipovak.properties.MessageResources</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>bienvenida</from-outcome>
            <to-view-id>/paginas/bienvenida.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>profesor</from-outcome>
            <to-view-id>/paginas/profListaGrupos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>estudiante</from-outcome>
            <to-view-id>/paginas/estSeleccionTest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>registroUsuario</from-outcome>
            <to-view-id>/paginas/registraUsuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/*</from-view-id>
        <navigation-case>
            <from-outcome>gestionarProgramas</from-outcome>
            <to-view-id>/paginas/adminGestionarProgramas.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>gestionarPeridos</from-outcome>
            <to-view-id>/paginas/adminGestionarPeridos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>gestionarGrupos</from-outcome>
            <to-view-id>/paginas/adminGestionarGrupos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>gestionarPreguntas</from-outcome>
            <to-view-id>/paginas/adminGestionarPreguntas.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>profesor</from-outcome>
            <to-view-id>/paginas/profListaGrupos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>estudiante</from-outcome>
            <to-view-id>/paginas/estSeleccionTest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/profListaGrupos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>veResultados</from-outcome>
            <to-view-id>/paginas/profVeResultados.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/profVeDetalleResultados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/paginas/profVeResultadosXEstudiante.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/profVeResultadosXEstudiante.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/paginas/profVeResultados.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>verResultadosIndividuales</from-outcome>
            <to-view-id>/paginas/profVeDetalleResultados.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/profVeResultados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>verResultadosXEstudiante</from-outcome>
            <to-view-id>/paginas/profVeResultadosXEstudiante.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/estSeleccionTest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>presentaTest</from-outcome>
            <to-view-id>/paginas/estPresentaTest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>verResultados</from-outcome>
            <to-view-id>/paginas/estVeResultados.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/registraUsuario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>registroExitoso</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>salir</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/profVeResultados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/paginas/profListaGrupos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/estPresentaTest.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>verResultados</from-outcome>
            <to-view-id>/paginas/estVeResultados.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/paginas/estSeleccionTest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/estVeResultados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/paginas/estSeleccionTest.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
